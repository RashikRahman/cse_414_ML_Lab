## Annotation
def annotate_bars(ax, thres):
  for patch in ax.patches:
    width = patch.get_width()
    if width>thres:
      ax.text(
              patch.get_x()+width/2, patch.get_y()+patch.get_height()/2, f'{width:.1%} ',
              va='center', ha='center', color='white'
          )
  return ax


